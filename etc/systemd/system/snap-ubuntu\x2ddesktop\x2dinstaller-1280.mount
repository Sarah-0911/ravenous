[Unit]
Description=Mount unit for ubuntu-desktop-installer, revision 1280
After=snapd.mounts-pre.target
Before=snapd.mounts.target
Before=local-fs.target

[Mount]
What=/var/lib/snapd/snaps/ubuntu-desktop-installer_1280.snap
Where=/snap/ubuntu-desktop-installer/1280
Type=fuse.snapfuse
Options=nodev,ro,x-gdu.hide,x-gvfs-hide,allow_other
LazyUnmount=yes

[Install]
WantedBy=snapd.mounts.target
WantedBy=multi-user.target
